<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>RegServlet</servlet-name>
    <servlet-class>com.event.RegServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AddEventServlet</servlet-name>
    <servlet-class>com.event.AddEventServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.event.LoginServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CheckSession</servlet-name>
    <servlet-class>com.event.CheckSession</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ExitSession</servlet-name>
    <servlet-class>com.event.ExitSession</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.event.MainServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RegServlet</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AddEventServlet</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CheckSession</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ExitSession</servlet-name>
    <url-pattern>/exitSession</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>

</web-app>
